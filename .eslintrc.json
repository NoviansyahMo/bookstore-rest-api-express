{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", { "code": 100 , "ignoreStrings": true}],
        "linebreak-style":"off",
        "no-unused-vars":"warn"
    }
}
